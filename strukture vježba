#include <iostream>

using namespace std;
struct osoba{
    string ime;
    string prezime;
    int god;
};
void ispis(osoba os){
    cout<<"O osobi: "<<endl;
        cout<<"Ime: "<<os.ime<<endl;
        cout<<"Prezime: "<<os.prezime<<endl;
        cout<<"Godiste: "<<os.god<<endl;
        cout<<endl;
}
int main()
{
    osoba polje[5];
    for(int i=0;i<5;i++){
        cout<<"Unesi ime osobe: "<<endl;
        cin>>polje[i].ime;
        cout<<"Unesi prezime osobe: "<<endl;
        cin>>polje[i].prezime;
        cout<<"Unesi godiste: "<<endl;
        cin>>polje[i].god;
        cout<<endl<<endl;
        ispis(polje[i]);
    }
    for(int i=0;i<5;i++){
            int najmanjii=i;
        for(int j=i;j<5;j++){
            if(polje[j].god<polje[najmanjii].god){
                najmanjii=j;
            }
        }
        swap(polje[i],polje[najmanjii]);
    }
    cout<<"ISPIS"<<endl;
    for(int i=0;i<5;i++){
        ispis(polje[i]);
    }
}
