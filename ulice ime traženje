
#include <iostream>

using namespace std;
struct osoba{
    string ime;
    string prezime;
    int god;
    string mjesto;
    string ulica;
    int kbr;
};
void ispis(osoba os){
    cout<<"O osobi: "<<endl;
        cout<<"Ime: "<<os.ime<<endl;
        cout<<"Prezime: "<<os.prezime<<endl;
        cout<<"Godiste: "<<os.god<<endl;
        cout<<"Mjesto: "<<os.mjesto<<endl;
        cout<<"Ulica: "<<os.ulica<<endl;
        cout<<"Kbr: "<<os.kbr<<endl;
        cout<<endl;
}
int main()
{
    osoba polje[3];
    string nime =" rtska";
    for(int i=0;i<3;i++){
        cout<<"Unesi ime osobe: "<<endl;
        cin>>polje[i].ime;
        cout<<"Unesi prezime osobe: "<<endl;
        cin>>polje[i].prezime;
        cout<<"Unesi godiste: "<<endl;
        cin>>polje[i].god;
        cout<<"Unesite mjesto: "<<endl;
        cin>>polje[i].mjesto;
        cout<<"Unesite ulicu: "<<endl;
        cin>>polje[i].ulica;
        cout<<"Unesite kbr: "<<endl;
        cin>>polje[i].kbr;
        cout<<endl<<endl;
        ispis(polje[i]);
    }
    cout<<endl;
    cout<<"Ispis"<<endl;
    for(int i=0;i<3;i++){
        if(polje[i].ulica == nime)

         ispis(polje[i]);





    }
}
